    "    profiles[name]['jobsInfo'] = {}\n",
    "    profiles[name]['eduInfo'] = {}\n",
    "    \n",
    "    # Content\n",
    "    try:\n",
    "        experiences = driver.find_element_by_css_selector('.background-section')\n",
    "    except:\n",
    "        experiences = ''\n",
